<AnchorPane fx:id="contentPane" prefHeight="500.0" prefWidth="500.0" styleClass="customStageBorder" stylesheets="@../css/application.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.controllers.ControllerCustomStage">
